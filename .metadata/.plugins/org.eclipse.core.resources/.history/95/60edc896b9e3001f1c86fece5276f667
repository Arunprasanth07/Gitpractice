package reCap;

public class looping {

	static int n;

	protected void weird(int n){
		if(n % 2 !=0){
			System.out.println("Weird");
		}else if(n>=2 && n<=5){
			System.out.println("Not Weird");
		} else if(n>=6 && n<=20){
			System.out.println("Weird");
		} else if(n>20){
			System.out.println("Not Weird");}   
	}

	public static void main(String[] args) {

		looping odd = new looping();
		odd.weird(3);
		odd.weird(24);

	}

}
